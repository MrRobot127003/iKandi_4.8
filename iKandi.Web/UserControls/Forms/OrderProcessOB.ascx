    function GetStitchTotal() {
        //debugger;
        var Rowscount = $('#<%=hdnStitchCount.ClientID%>').val();
        if (Rowscount == '') {
            Rowscount = 0;
        }
        Rowscount = parseInt(Rowscount) + 3;
        var StitchSamTotal = 0;
        var idAdd = '';
        for (var Ids = 0; Ids < Rowscount; Ids++) {
            if (Ids < 10) {
                idAdd = '0' + Ids;
            }
            else {
                idAdd = Ids;
            }
            StitchSamTotal = $("#<%= GrdStichingManPower.ClientID %> input[id*='ctl" + idAdd + "_hdnStitchSamTotal" + "']").val();
        }
        //debugger;
        if (typeof (StitchSamTotal) === "undefined") {
            StitchSamTotal = '';
        }
        $("#<%=lblTotalStitchingSam.ClientID%>")[0].innerHTML = StitchSamTotal;
    }



